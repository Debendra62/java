package com.Basic_java_class_2;

public class NarrowingDemo {

	public static void main(String[] args) {
	      double num1=2.5;
	      int num2=(int)num1;
	      System.out.println("The new int is "+num2);
	      double num3=3.54;
	      byte num4=(byte)num3;
	      System.out.println("The new byte is "+num4);
	      double num5=2.5;
	      float num6=(float)num5;
	      System.out.println("The new float is "+num6);
	      float num7=3.54f;
	      long num8=(long)num7;
	      System.out.println("The new long is "+num8);
		
}
}
