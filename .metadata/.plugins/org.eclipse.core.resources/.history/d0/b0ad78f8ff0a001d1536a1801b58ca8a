package com.Basic_java_class_1;

public class InstanceVariableDemo {
        private int num1;
        private int num2;
        private int sum;
        private int mul;
        
        InstanceVariableDemo(int num1,int num2) {
			this.num1=num1;
			this.num2=num2;
			
		}
	
	    public static void main(String[] args) {
	        System.out.println("HI");
}
}
