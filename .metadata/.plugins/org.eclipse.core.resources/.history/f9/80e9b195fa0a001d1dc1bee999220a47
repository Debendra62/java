package com.Basic_java_class_1;

public class LocalVariableDemo {
      LocalVariableDemo(){
    	   int num1=10;
    	   int num2=20;
    	   int sum=0;
    	   sum=num1+num2;
    	   
    	   System.out.println("Sum of "+num1+" and "+num2+" = "+sum);
      }
	public static void main(String[] args) {
		new LocalVariableDemo();
	}
}
